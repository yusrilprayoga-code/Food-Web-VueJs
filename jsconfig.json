{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
    "vue/multi-word-component-names": ["error", {
      "ignores": [
      ]
    }],
    "vue/no-unused-components": ["error", {
      "ignoreWhenBindingPresent": true
    }]
}
